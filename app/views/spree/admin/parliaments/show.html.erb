<% content_for :page_title do %>
  <%= link_to(Spree.t(:name, scope: :owls), admin_parliaments_url) %> / <%= @parliament.name %>
<% end %>

<% content_for :page_actions do %>
  <%= link_to_with_icon 'translate', nil, admin_translations_path('parliament', @parliament.id), title: Spree.t(:'i18n.translations'), class: 'btn btn-sm btn-primary' %>
<% end if can?(:translate, Spree::Parliament) %>

<% if @owls.any? %>
  <%= debug @owls %>
<% end %>